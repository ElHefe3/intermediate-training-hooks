{
  "name": "cool-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "prepare": "husky install && husky set .husky/pre-commit \"yarn pre-commit\"",
    "pre-commit": "lint-staged && yarn build"
  },
  "dependencies": {
    "@codehesion-za/headless": "0.8.4",
    "@headlessui/react": "1.7.16",
    "@heroicons/react": "2.0.18",
    "@tanstack/react-query": "4.32.1",
    "@tanstack/react-table": "8.9.3",
    "axios": "1.4.0",
    "axios-auth-refresh": "3.3.6",
    "formik": "2.4.3",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-activity": "2.1.3",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "react-router-dom": "6.14.2",
    "react-select": "5.7.4",
    "zod": "3.21.4",
    "zod-formik-adapter": "1.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.4",
    "@tanstack/react-query-devtools": "4.32.1",
    "@testing-library/react": "14.0.0",
    "@types/lodash": "4.14.196",
    "@types/node": "18.0.0",
    "@types/react-dom": "18.2.7",
    "@types/react-table": "7.7.12",
    "@types/tailwindcss": "3.0.10",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "@vitejs/plugin-react": "1.3.2",
    "autoprefixer": "10.4.7",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jsdom": "22.0.0",
    "lint-staged": "13.2.3",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite": "4.4.5",
    "vitest": "0.31.0"
  },
  "imports": {
    "@project/*": "./src"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
