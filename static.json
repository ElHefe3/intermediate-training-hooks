{
    "root": "./dist",
    "https_only": true,
  
    "headers": {
      "/**": {
        "Strict-Transport-Security": "max-age=7776001"
      },
  
      "/": {
        "Cache-Control": "no-store, no-cache",
        "Strict-Transport-Security": "max-age=7776002"
      },
  
      "/assets/**": {
        "Cache-Control": "public, max-age=31536000",
        "Access-Control-Allow-Origin": "*"
      },
  
      "/manifest.json": {
        "Cache-Control": "no-store, no-cache",
        "Access-Control-Allow-Origin": "*"
      }
    },
  
    "routes": {
      "/assets/*": "/assets/",
      "/**": "index.html"
    }
}